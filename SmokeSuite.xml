<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Calculator All Test Suite">
    <groups>
        <run>
            <include name="smoke"/>
        </run>
    </groups>
    <listeners>
        <listener class-name="org.listeners.TestListeners"/>
    </listeners>
    <test name="CalculatorTest" parallel="methods" thread-count="5">
        <classes>
            <class name="CalculatorTest"/>
        </classes>
    </test>
</suite>