<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Calculator All Test Suite">
    <listeners>
        <listener class-name="org.listeners.TestListeners"/>
    </listeners>
    <test name="CalculatorTest" parallel="methods" thread-count="10">
        <classes>
            <class name="CalculatorTest">
                <methods>
                    <exclude name="testMultiplyDoubleNum"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>